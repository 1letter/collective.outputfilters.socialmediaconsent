     xmlns:metal="http://xml.zope.org/namespaces/metal"
     xmlns:tal="http://xml.zope.org/namespaces/tal"
     lang="en"
     xml:lang="en"
     tal:omit-tag=""
     i18n:domain="collective.outpufilters.socialmediaconsent"
>
  <div class="form-check placeholder-socialmedia-consent placeholder-socialmedia-consent-youtube"
       data-cos="${options/cos}"
  >
